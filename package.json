{
  "name": "huddle-landing-page-with-single-introductory-section",
  "version": "1.0.0",
  "scripts": {
    "start": "concurrently --kill-others \"npm:watch\" \"eleventy --serve\"",
    "build": "npm run css && eleventy",
    "css": "postcss src/css/global.css -o dist/global.css",
    "watch": "postcss src/css/global.css -o dist/global.css -w"
  },
  "author": "Josh Javier",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@prettier/plugin-xml": "^2.2.0",
    "concurrently": "^8.0.1",
    "cssnano": "^6.0.1",
    "djlint": "^1.29.0",
    "modern-css-reset": "^1.4.0",
    "postcss": "^8.4.23",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-import-ext-glob": "^2.1.1",
    "postcss-nested": "^6.0.1",
    "prettier": "2.8.8",
    "serve": "^14.2.0"
  }
}
